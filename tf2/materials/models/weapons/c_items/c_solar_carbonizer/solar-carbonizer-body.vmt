"VertexlitGeneric"
{
	"$basetexture" "models/weapons/c_items/c_solar_carbonizer/solar-carbonizer-body"
	"$bumpmap" "models/weapons/c_items/c_solar_carbonizer/solar-carbonizer-normals"
	"$envmaptint" "[.8 .75 .45]"
	"$normalmapalphaenvmapmask" "1" 
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "15"
	"$phongfresnelranges"	"[.2 1 4]"
	
	"$selfillum" "1"
	"$selfillumtint" "[1.5 1.5 1.5]"
	"$selfillummask" "models/weapons/c_items/c_solar_carbonizer/solar-carbonizer-body-light"
	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".2"
	
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$sheenmap" "cubemaps/cubemap_sheen001"
	"$sheenmapmask" "effects/animatedsheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
