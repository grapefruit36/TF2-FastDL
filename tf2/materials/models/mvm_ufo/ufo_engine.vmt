"UnLitTwoTexture"
{
	"$basetexture" "models/mvm_ufo/ufo_engine_1.vtf"
	"$texture2" "models/mvm_ufo/ufo_engine_2.vtf"
	"$model" "1"
	"%keywords" "tf"

	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" .2
			"texturescrollangle" 180
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" .05
			"texturescrollangle" 90
		}
		"Sine"
		{
			"Sineperiod" "1"
			"SineMax" ".9"
			"SineMin" ".8"
			"resultVar" "$color"
		}
	}
}